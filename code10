
//largest of three number
#include <stdio.h>
int main ()
{ 
    int a,b,c;
    printf ("enter number1");
    scanf ("%d",&a);
    printf ("enter number2");
    scanf ("%d",&b);
    printf ("enter number3");
    scanf ("%d",&c);
     if (a>b && a>c)
       printf ("%d",a);
    else if (b>c && b>a)
       printf ("%d",b);
    else 
       printf ("%d",c);
    return 0;
}
